{
    "manifest_version": 3,
    "name": "Fake News Detector",
    "version": "1.0",
    "description": "Paste a news article and detect if it's real or fake.",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  